<PreferenceScreen xmlns:android="http://schemas.android.com/apk/res/android"
        android:title="@string/about_settings">

	<!-- Turbo ROM Logo -->
        <PreferenceScreen android:key="turbo_logo"
		android:enabled="false"
		android:layout="@layout/turbo_logo_row" />

        <!-- System update settings - launches activity 
        <PreferenceScreen android:key="system_update_settings"
                android:title="@string/system_update_settings_list_item_title"
                android:summary="@string/system_update_settings_list_item_summary">
            <intent android:action="android.settings.SYSTEM_UPDATE_SETTINGS" />
        </PreferenceScreen>


        <PreferenceScreen android:key="additional_system_update_settings"
                          android:title="@string/additional_system_update_settings_list_item_title">
            <intent android:action="android.intent.action.MAIN"
                    android:targetPackage="@string/additional_system_update"
                    android:targetClass="@string/additional_system_update_menu" />
        </PreferenceScreen> -->

        <!-- Turbo OTA -->
        <PreferenceScreen
                android:key="turboota"
                android:title="@string/turboota_title">
                <intent
                    android:action="android.intent.action.MAIN"
                    android:targetPackage="com.turborom.turboota"
                    android:targetClass="com.turborom.turboota.MainActivity" />
        </PreferenceScreen>

        <!-- Changelog -->
 	<PreferenceScreen 
               android:key="translator"
               android:title="@string/changelog_turbo_title"
               android:fragment="com.android.settings.turbo.Changelog" />
 
        <!-- Device status - launches activity -->
        <PreferenceScreen android:key="status_info"
                android:title="@string/device_status"
                android:summary="@string/device_status_summary">
            <intent android:action="android.intent.action.MAIN"
                    android:targetPackage="com.android.settings"
                    android:targetClass="com.android.settings.deviceinfo.Status" />
        </PreferenceScreen>

        <!-- Manual -->
        <PreferenceScreen
                android:key="manual"
                android:title="@string/manual">
            <intent android:action="android.settings.SHOW_MANUAL" />
        </PreferenceScreen>

        <PreferenceScreen
                android:key="regulatory_info"
                android:title="@string/regulatory_information">
            <intent android:action="android.settings.SHOW_REGULATORY_INFO" />
        </PreferenceScreen>

        <!-- Feedback on the device  -->
        <PreferenceScreen android:key="device_feedback"
                android:title="@string/device_feedback">
        </PreferenceScreen>

        <!-- Device hardware model -->
        <Preference android:key="device_model"
                style="?android:preferenceInformationStyle"
                android:title="@string/model_number"
                android:summary="@string/device_info_default"/>

        <!-- Device CPU info -->
        <Preference android:key="device_cpu"
                style="?android:preferenceInformationStyle"
                android:title="@string/cpu_info"
                android:summary="@string/device_info_default"/>

        <!-- Device RAM info -->
        <Preference android:key="device_memory"
                style="?android:preferenceInformationStyle"
                android:title="@string/mem_info"
                android:summary="@string/device_info_default"/>

        <!-- Device firmware version -->
        <Preference android:key="firmware_version"
                style="?android:preferenceInformationStyle"
                android:title="@string/firmware_version"
                android:summary="@string/device_info_default"/>

        <!-- Device build id -->
        <Preference android:key="build_id" 
                style="?android:preferenceInformationStyle"
                android:title="@string/build_id"
                android:summary="@string/device_info_default"/>

        <!-- Security patch level -->
        <Preference android:key="security_patch"
                style="?android:preferenceInformationStyle"
                android:title="@string/security_patch"
                android:summary="@string/device_info_default"/>

        <!-- Turbo firmware version -->
        <Preference android:key="turbo_version"
                style="?android:preferenceInformationStyle"
                android:title="@string/turbo_version_title"
                android:summary="@string/turbo_version_default"/>

        <!-- GCC build info for Android -->
        <Preference android:key="uber_android"
                style="?android:preferenceInformationStyle"
                android:title="@string/uber_android"
                android:summary="@string/uber_android_default" />

	<!-- GCC build info for kernel -->
        <Preference android:key="uber_kernel"
                style="?android:preferenceInformationStyle"
                android:title="@string/uber_kernel"
                android:summary="@string/uber_kernel_default" />

	<!-- GCC build flags info -->
        <Preference android:key="uber_flags"
                style="?android:preferenceInformationStyle"
                android:title="@string/uber_flags"
                android:summary="@string/uber_flags_default" />

        <!-- Device Kernel version -->
        <Preference android:key="kernel_version" 
                style="?android:preferenceInformationStyle"
                android:title="@string/kernel_version"
                android:summary="@string/device_info_default"/>
                
        <!-- Build date -->
        <Preference android:key="build_date"
                style="?android:preferenceInformationStyle"
                android:title="@string/build_date"
                android:summary="@string/device_info_default" />

        <!-- Detailed build version -->
        <Preference android:key="build_number"
                style="?android:preferenceInformationStyle"
                android:title="@string/build_number"
                android:summary="@string/device_info_default"/>

        <!-- Device FCC equipment id -->
        <Preference android:key="fcc_equipment_id"
                style="?android:preferenceInformationStyle"
                android:title="@string/fcc_equipment_id"
                android:summary="@string/device_info_default"/>

        <!-- Device Baseband version -->
        <Preference android:key="baseband_version"
                style="?android:preferenceInformationStyle"
                android:title="@string/baseband_version"
                android:summary="@string/device_info_default"/>

        <!-- SELinux status information -->
        <Preference android:key="selinux_status"
                style="?android:preferenceInformationStyle"
                android:title="@string/selinux_status"
                android:summary="@string/selinux_status_enforcing"/>

</PreferenceScreen>
